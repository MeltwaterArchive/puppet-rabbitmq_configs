{
  "name": "puppet-rabbitmq_configs",
  "version": "0.0.1",
  "author": "Simone Sciarrati",
  "summary": "Uses the types defined in the RabbitMQ module to configure some aspects of RabbitMQ via hiera (vhosts,echanges,queues,users,permissions,parameters,policies).",
  "license": "Apache-2.0",
  "source": "https://github.com/meltwater/puppet-rabbitmq_configs",
  "project_page": "https://github.com/meltwater/puppet-rabbitmq_configs",
  "issues_url": "",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">=3.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=3.x"
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/rabbitmq","version_requirement":">=5.3.1"}
  ]
}
